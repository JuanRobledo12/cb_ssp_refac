	data sector="waso_pollution_cost_factors" {
			var_node difference_variable=r"qty_waso_open_dump_*" multiplier=-115 multiplier_unit=r"$/ton" annual_change=1 output_variable_name=r"cb:waso:env_pollution:open_dumping:X" output_display_name=r"Avoided cost of waste pollution (open dumping)" sum=1 display_notes=r"Wilson et al. (2015) suggest a conservative cost of $20-50/capita/year from unmanaged waste, and describe average waste of 220kg/cap/year among the poorest. We calculate costs assuming $20/capita and 0.22 ton/capita, adjusted from 2015 to 2019 dollars." output_variable_name=r"cb:waso:env_pollution:open_dumping:X" internal_notes=null

	}
	data sector="soil_nitrogen_and_lime_cost_factors" {
			var_node difference_variable=r"demand_soil_fertilizer_n_kt" multiplier=-200000 multiplier_unit=r"$/kt" annual_change=1 output_variable_name=r"cb:soil:technical_cost:agriculture_inputs:fertilizer" output_display_name=r"Cost savings (avoided fertilizer)" sum=0 display_notes=r"We use the average costs per ton of urea over the past five years as a cost of fertilier. This cost is available for download at https://www.indexmundi.com/commodities/?commodity=urea&months=60, accessed June 17, 2023." output_variable_name=r"cb:soil:technical_cost:agriculture_inputs:fertilizer" internal_notes=null

			var_node difference_variable=r"demand_soil_fertilizer_n_kt" multiplier=-60000 multiplier_unit=r"$/kt" annual_change=1 output_variable_name=r"cb:soil:land_pollution:N_leaching:fertilizer" output_display_name=r"Cost savings (avoided fertilizer)" sum=0 display_notes=r"Good and Beatty (2011, Box 2 and Tbale 4) estimate the total environmental cost of nitrate leaching and runoff to be 44% of the total cost of excess fertilizer applied, of which 70% of is attributed to nitrate leaching and runoff. The value of avoided nitrate leaching and runoff is estimated at 30% of the per-ton cost of fertilizer in the prior table." output_variable_name=r"cb:soil:land_pollution:N_leaching:fertilizer" internal_notes=null

	}
	data sector="ippu_avoided_production_cost_factors" {
